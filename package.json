{
  "name": "@commercelayer/contentful-commerce",
  "version": "0.1.0",
  "license": "MIT",
  "author": {
    "name": "Alex Canessa",
    "email": "alex@commercelayer.io"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@contentful/app-sdk": "^4.24.0",
    "@contentful/f36-components": "4.58.4",
    "@contentful/f36-tokens": "4.0.4",
    "@contentful/forma-36-tokens": "^0.11.2",
    "@contentful/react-apps-toolkit": "1.2.16",
    "@emotion/css": "^11.11.2",
    "contentful-management": "10.46.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "install-ci": "npm ci",
    "lint": "eslint .",
    "format": "prettier --write --cache --ignore-unknown .",
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "deploy": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id $DEFINITIONS_ORG_ID --definition-id 5AuHBmPgmRNIwuxHhVu9PD --token $CONTENTFUL_CMA_TOKEN"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commercelayer/eslint-config-ts-react": "^1.3.0",
    "@contentful/app-scripts": "1.13.2",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.1.2",
    "@tsconfig/create-react-app": "2.0.2",
    "@types/jest": "^29.5.11",
    "@types/node": "16.18.74",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "cross-env": "7.0.3",
    "eslint": "^8.56.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "typescript": "4.9.5"
  },
  "homepage": "."
}
